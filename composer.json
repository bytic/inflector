{
  "name": "bytic/inflector",
  "type": "library",
  "description": "Php Framework",
  "homepage": "https://github.com/bytic/inflector",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nip\\Inflector\\": "src"
    },
    "classmap": [
      "src"
    ],
    "files": [
      "src/functions/application.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Nip\\Inflector\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.0",
    "bytic/container": "^1.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0",
    "phpunit/phpunit": "^6.2",
    "mockery/mockery": "^0.9"
  }
}
